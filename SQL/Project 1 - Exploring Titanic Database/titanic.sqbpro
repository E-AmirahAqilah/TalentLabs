    (Parch + SibSp) AS FamilySize, 
    COUNT(*) AS Total_Passengers,
    SUM(CASE WHEN Survived = 1 THEN 1 ELSE 0 END) AS Total_Survived,
    SUM(CASE WHEN Survived = 0 THEN 1 ELSE 0 END) AS Total_Unsurvived,
    AVG(Survived) * 100 AS Survival_Rate,
    (SUM(CASE WHEN Survived = 0 THEN 1 ELSE 0 END) * 100.0 / COUNT(*)) AS Unsurvival_Rate
FROM 
    passengers
GROUP BY 
    FamilySize
ORDER BY 
    FamilySize;




















